@model List<DAL.Domains.Category>
<div class="row">
    <div class="col-sm-12">
        <ul class="filter font-alt" id="filters">
            <li><a class="current wow fadeInUp" href="#" data-filter="*">All</a></li>
            @foreach (var ctgr in Model)
            {
                <li>
                    <a class="wow fadeInUp"
                       data-wow-delay="0.2s"
                       asp-controller="Home"
                       asp-action="Index"
                       asp-route-id="@ctgr.Id">@ctgr.Name</a>
                </li>
            }
        </ul>
    </div>
</div>

