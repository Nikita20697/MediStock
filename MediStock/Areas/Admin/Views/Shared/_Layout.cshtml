<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>MediStock-Admin</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="~/Content/Themes/Admin/assets/img/favicon-32x32.png" type="image/x-icon" />

    <!-- Fonts and icons -->
    <link href="~/Content/Themes/Admin/assets/css/fonts.css" rel="stylesheet" />

    <!-- CSS Files -->
    <link rel="stylesheet" href="~/Content/Themes/Admin/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/Themes/Admin/assets/css/atlantis.min.css">
    <link href="~/Content/Common/cropper/cropper.min.css" rel="stylesheet" />
    <link href="~/Content/Common/air-datepicker/css/datepicker.min.css" rel="stylesheet" />

    <!-- Jquer Initial Load -->
    <script src="~/Content/Themes/Admin/assets/js/core/jquery.3.2.1.min.js"></script>

</head>
<body>
    <div class="wrapper">
        <div class="main-header">
            <!-- Logo Header -->
            <div class="logo-header" data-background-color="blue">

                <a href="@Url.Action("Index", "Dashboard")" class="logo">
                    @*<img src="~/Content/Themes/Admin/assets/img/logo.png" alt="navbar brand" class="navbar-brand">*@
                </a>

                <button class="navbar-toggler sidenav-toggler ml-auto" type="button" data-toggle="collapse" data-target="collapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon">
                        <i class="icon-menu"></i>
                    </span>
                </button>
                <button class="topbar-toggler more"><i class="icon-options-vertical"></i></button>
                <div class="nav-toggle">
                    <button class="btn btn-toggle toggle-sidebar">
                        <i class="icon-menu"></i>
                    </button>
                </div>

            </div>
            <!-- End Logo Header -->
            <!-- Navbar Header -->
            <nav class="navbar navbar-header navbar-expand-lg" data-background-color="blue2">

                <div class="container-fluid">
                    <div class="collapse" id="search-nav">
                        <form class="navbar-left navbar-form nav-search mr-md-3">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <button type="submit" class="btn btn-search pr-1">
                                        <i class="fa fa-search search-icon"></i>
                                    </button>
                                </div>
                                <input type="text" placeholder="Search ..." class="form-control">
                            </div>
                        </form>
                    </div>
                    <ul class="navbar-nav topbar-nav ml-md-auto align-items-center">
                        <li class="nav-item toggle-nav-search hidden-caret">
                            <a class="nav-link" data-toggle="collapse" href="#search-nav" role="button" aria-expanded="false" aria-controls="search-nav">
                                <i class="fa fa-search"></i>
                            </a>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a class="nav-link dropdown-toggle" href="javascript:void(0)" id="messageDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-envelope"></i>
                            </a>
                            <ul class="dropdown-menu messages-notif-box animated fadeIn" aria-labelledby="messageDropdown">
                                <li>
                                    <div class="dropdown-title d-flex justify-content-between align-items-center">
                                        Messages
                                        <a href="javascript:void(0)" class="small">Mark all as read</a>
                                    </div>
                                </li>
                                <li>
                                    <div class="message-notif-scroll scrollbar-outer">
                                        <div class="notif-center">
                                            <a href="javascript:void(0)">
                                                <div class="notif-img">
                                                    <img src="~/Content/Themes/Admin/assets/img/jm_denis.jpg" alt="Img Profile">
                                                </div>
                                                <div class="notif-content">
                                                    <span class="subject">Raj Pawar</span>
                                                    <span class="block">
                                                        How are you ?
                                                    </span>
                                                    <span class="time">5 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-img">
                                                    <img src="~/Content/Themes/Admin/assets/img/chadengle.jpg" alt="Img Profile">
                                                </div>
                                                <div class="notif-content">
                                                    <span class="subject">Meet Parmar</span>
                                                    <span class="block">
                                                        Ok, Thanks !
                                                    </span>
                                                    <span class="time">12 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-img">
                                                    <img src="~/Content/Themes/Admin/assets/img/mlane.jpg" alt="Img Profile">
                                                </div>
                                                <div class="notif-content">
                                                    <span class="subject">Rakesh Girase</span>
                                                    <span class="block">
                                                        Ready for the meeting today...
                                                    </span>
                                                    <span class="time">12 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-img">
                                                    <img src="~/Content/Themes/Admin/assets/img/talha.jpg" alt="Img Profile">
                                                </div>
                                                <div class="notif-content">
                                                    <span class="subject">Bhautik Patel</span>
                                                    <span class="block">
                                                        Hi, Apa Kabar ?
                                                    </span>
                                                    <span class="time">17 minutes ago</span>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </li>
                                <li>
                                    <a class="see-all" href="javascript:void(0);">See all messages<i class="fa fa-angle-right"></i> </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a class="nav-link dropdown-toggle" href="javascript:void(0)" id="notifDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-bell"></i>
                                <span class="notification">4</span>
                            </a>
                            <ul class="dropdown-menu notif-box animated fadeIn" aria-labelledby="notifDropdown">
                                <li>
                                    <div class="dropdown-title">You have 4 new notification</div>
                                </li>
                                <li>
                                    <div class="notif-scroll scrollbar-outer">
                                        <div class="notif-center">
                                            <a href="javascript:void(0)">
                                                <div class="notif-icon notif-primary"> <i class="fa fa-user-plus"></i> </div>
                                                <div class="notif-content">
                                                    <span class="block">
                                                        New user registered
                                                    </span>
                                                    <span class="time">5 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-icon notif-success"> <i class="fa fa-comment"></i> </div>
                                                <div class="notif-content">
                                                    <span class="block">
                                                        Ilyas commented on Admin
                                                    </span>
                                                    <span class="time">12 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-img">
                                                    <img src="~/Content/Themes/Admin/assets/img/profile2.jpg" alt="Img Profile">
                                                </div>
                                                <div class="notif-content">
                                                    <span class="block">
                                                        Reza send messages to you
                                                    </span>
                                                    <span class="time">12 minutes ago</span>
                                                </div>
                                            </a>
                                            <a href="javascript:void(0)">
                                                <div class="notif-icon notif-danger"> <i class="fa fa-heart"></i> </div>
                                                <div class="notif-content">
                                                    <span class="block">
                                                        Farrah liked Admin
                                                    </span>
                                                    <span class="time">17 minutes ago</span>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </li>
                                <li>
                                    <a class="see-all" href="javascript:void(0);">See all notifications<i class="fa fa-angle-right"></i> </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="javascript:void(0)" aria-expanded="false">
                                <div class="avatar-sm">
                                    <img src="~/Content/Themes/Admin/assets/img/rajAdmin.png" alt="..." class="avatar-img rounded-circle">
                                </div>
                            </a>
                            <ul class="dropdown-menu dropdown-user animated fadeIn">
                                <div class="dropdown-user-scroll scrollbar-outer">
                                    <li>
                                        <div class="user-box">
                                            <div class="avatar-lg"><img src="~/Content/Themes/Admin/assets/img/rajAdmin.png" alt="image profile" class="avatar-img rounded"></div>
                                            <div class="u-text">
                                                <h4>Raj</h4>
                                                <p class="text-muted">er.rajpawar@gmail.com</p><a href="@Url.Action("Profile", "User")" class="btn btn-xs btn-secondary btn-sm">View Profile</a>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="dropdown-divider"></div>
                                        <a class="dropdown-item" href="javascript:void(0)">My Profile</a>
                                        <a class="dropdown-item" href="javascript:void(0)">My Balance</a>
                                        <a class="dropdown-item" href="javascript:void(0)">Logout</a>
                                    </li>
                                </div>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
            <!-- End Navbar -->
        </div>

        <!-- Sidebar -->
        <div class="sidebar sidebar-style-2">
            <div class="sidebar-wrapper scrollbar scrollbar-inner">
                <div class="sidebar-content">
                    <div class="user">
                        <div class="avatar-sm float-left mr-2">
                            <img src="~/Content/Themes/Admin/assets/img/rajAdmin.png" alt="..." class="avatar-img rounded-circle">
                        </div>
                        <div class="info">
                            <a href="javascripr:void(0)">
                                <span>
                                    Raj
                                    <span class="user-level">Administrator</span>
                                </span>
                            </a>
                        </div>
                    </div>
                    <ul class="nav nav-primary">
                        <li class="nav-item">
                            <a id="dashboard-sidebar-id" href="@Url.Action("Index", "Dashboard")">
                                <i class="fas fa-home"></i>
                                <p>Dashboard</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="category-sidebar-id" href="@Url.Action("List", "Category")">
                                <i class="fas fa-list"></i>
                                <p>Categories</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="medicine-sidebar-id" href="@Url.Action("List", "Medicine")">
                                <i class="fas fa-file-medical"></i>
                                <p>Medicines</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="user-sidebar-id" href="@Url.Action("List", "AdminUser")">
                                <i class="fas fa-users-cog"></i>
                                <p>Users</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="order-sidebar-id" href="@Url.Action("List", "Order")">
                                <i class="fas fa-dot-circle"></i>
                                <p>Orders</p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="payment-sidebar-id" href="@Url.Action("Index", "Payment")">
                                <i class="fas fa-rupee-sign"></i>
                                <p>Payments</p>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- End Sidebar -->

        <div class="main-panel">
            <div class="content">
                @RenderBody()
            </div>
            <footer class="footer">
                <div class="container-fluid">
                    <div class="copyright ml-auto">
                        Copyright @@ <a href="@Url.Action("Index", "Dashboard")">MediStock</a>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <!--   Core JS Files   -->
    <script src="~/Content/Themes/Admin/assets/js/core/popper.min.js"></script>
    <script src="~/Content/Themes/Admin/assets/js/core/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="~/Content/Themes/Admin/assets/js/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>

    <!-- jQuery Scrollbar -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>

    <!-- Chart JS -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/chart.js/chart.min.js"></script>

    <!-- jQuery Sparkline -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/jquery.sparkline/jquery.sparkline.min.js"></script>

    <!-- Chart Circle -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/chart-circle/circles.min.js"></script>

    <!-- Datatables -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/datatables/datatables.min.js"></script>

    <!-- Bootstrap Notify -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- Sweet Alert -->
    <script src="~/Content/Themes/Admin/assets/js/plugin/sweetalert/sweetalert.min.js"></script>

    <!-- Atlantis JS -->
    <script src="~/Content/Themes/Admin/assets/js/atlantis.min.js"></script>

    <!-- Cropper JS -->
    @*https://github.com/fengyuanchen/cropperjs/blob/master/README.md*@
    <script src="~/Content/Common/cropper/cropper.min.js"></script>

    <!-- VUE QR-CODE -->
    @*https://fengyuanchen.github.io/vue-qrcode/*@
    <script src="~/Content/Common/vue-qrcode/vue-qrcode.min.js"></script>

    <!-- VUE QR-CODE -->
    @*https://fengyuanchen.github.io/vue-countdown/*@
    <script src="~/Content/Common/vue-countdown/vue-countdown.min.js"></script>

    @*http://t1m0n.name/air-datepicker/docs/*@
    <script src="~/Content/Common/air-datepicker/js/datepicker.min.js"></script>

</body>
</html>